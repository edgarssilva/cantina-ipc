{
  "expo": {
    "name": "Cantinas IPC",
    "slug": "cantinas-ipc",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#525EA6"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/icon.png",
        "backgroundColor": "#525EA6"
      },
      "package": "pt.bigbrains.cantinaipc"
    },
    "web": {
      "favicon": "./src/assets/icon.png"
    }
  }
}
